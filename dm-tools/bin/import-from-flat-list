#!/bin/env python

import sys
from path import Path
import sqlite3


def main():
    input_path = Path(sys.argv[1])
    output_path = sys.argv[2]
    con = sqlite3.connect(output_path)
    con.executemany(
        """
      INSERT OR IGNORE INTO entries(entry)
        VALUES(?)
      """,
        ((x,) for x in input_path.lines(retain=False)),
    )
    con.commit()
    con.close()


if __name__ == "__main__":
    main()
